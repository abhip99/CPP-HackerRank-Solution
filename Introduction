#include <cmath>
#include <cstdio>
#include <vector>
#include <iostream>
#include <algorithm>
using namespace std;


int main() {
    int a[1000];
    int n;
    cin>>n;
    if(n>=1 && n<=1000)
    {
        for(int i=0; i<n; i++)
        {
            cin>>a[i];
        }
        for(int i=n;i>=1;i--)
        {
            cout<<a[i-1];
            cout<<" ";
        }
    }
    /* Enter your code here. Read input from STDIN. Print output to STDOUT */   
    return 0;
}
